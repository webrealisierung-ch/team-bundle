{
  "name": "wr/team-bundle",
  "type": "contao-bundle",
  "description": "Team bundle to show the employees of a company for Contao Open Source CMS",
  "keywords": ["team","employees","category","contao"],
  "homepage": "https://github.com/webrealisierung-ch/team-bundle",
  "license": "LGPL-3.0",
  "authors": [
    {
      "name": "Daniel Steuri ",
      "email": "mail@webrealisierung.ch",
      "homepage": "https://webrealisierung.ch"
    }
  ],
  "require": {
    "php": "^7.0",
    "contao/core-bundle": "^4.4"
  },
  "autoload": {
    "psr-4": {
      "Wr\\TeamBundle\\": "src/"
    },
    "classmap": [
      "src/Resources/contao/"
    ],
    "exclude-from-classmap": [
      "src/Resources/contao/config/",
      "src/Resources/contao/dca/",
      "src/Resources/contao/languages/",
      "src/Resources/contao/templates/"
    ]
  },
  "extra": {
    "contao-manager-plugin": "Wr\\TeamBundle\\ContaoManager\\Plugin"
  }
}
